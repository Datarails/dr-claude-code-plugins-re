{
  "name": "datarails-finance-os",
  "version": "1.0.0",
  "description": "Datarails Finance OS integration for Claude Code - analyze financial data, detect anomalies, and query tables with multi-account support",
  "author": {
    "name": "Datarails",
    "email": "support@datarails.com"
  },
  "homepage": "https://datarails.com",
  "repository": "https://github.com/Datarails/dr-claude-code-plugins",
  "license": "MIT",
  "keywords": [
    "datarails",
    "finance",
    "fpna",
    "data-analysis",
    "anomaly-detection",
    "multi-account"
  ],
  "mcpServers": {
    "datarails-finance-os": {
      "command": "uv",
      "args": ["--directory", "${pluginDir}/mcp-server", "run", "datarails-mcp", "serve"],
      "env": {
        "DATARAILS_CONFIG_DIR": "${pluginDir}/config"
      }
    }
  },
  "skills": [
    "skills/auth",
    "skills/tables",
    "skills/profile",
    "skills/anomalies",
    "skills/query"
  ],
  "agents": [
    "agents/finance-analyst.md"
  ]
}
