{
  "name": "datarails-finance-os",
  "version": "1.1.0",
  "description": "Datarails Finance OS integration for Claude Code and Cowork - analyze financial data, detect anomalies, and generate insights. Works with browser-based authentication (no CLI required for Cowork users).",
  "author": {
    "name": "Datarails",
    "email": "support@datarails.com"
  },
  "homepage": "https://datarails.com",
  "repository": "https://github.com/Datarails/dr-claude-code-plugins",
  "license": "MIT",
  "keywords": [
    "datarails",
    "finance",
    "fpna",
    "data-analysis",
    "anomaly-detection",
    "multi-account",
    "cowork",
    "financial-analysis"
  ],
  "mcpServers": {
    "datarails-finance-os": {
      "command": "uv",
      "args": ["--directory", "${CLAUDE_PLUGIN_ROOT}/mcp-server", "run", "datarails-mcp", "serve"],
      "env": {
        "DATARAILS_CONFIG_DIR": "${CLAUDE_PLUGIN_ROOT}/config"
      }
    }
  },
  "commands": [
    "commands/login.md",
    "commands/financial-summary.md",
    "commands/expense-analysis.md",
    "commands/revenue-trends.md",
    "commands/budget-comparison.md",
    "commands/test-api.md",
    "commands/data-check.md",
    "commands/explore-tables.md"
  ],
  "skills": [
    "skills/auth",
    "skills/test",
    "skills/tables",
    "skills/profile",
    "skills/anomalies",
    "skills/query",
    "skills/extract",
    "skills/learn",
    "skills/intelligence",
    "skills/anomalies-report",
    "skills/insights",
    "skills/reconciliation",
    "skills/dashboard",
    "skills/forecast-variance",
    "skills/audit",
    "skills/departments"
  ],
  "agents": [
    "agents/finance-analyst.md"
  ]
}
